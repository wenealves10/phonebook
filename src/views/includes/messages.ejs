<% if(errors.length > 0){ %> 
    <div class="row">
        <div class="col my-3">
            <% errors.forEach(erro => {%> 
                <div class="alert alert-danger text-center">
                    <%= erro %>
                </div>
            <% }) %> 
            
        </div>
    </div>
<% } %> 
<% if(success.length > 0){ %> 
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success text-center">
                <% success.forEach(succe => {%> 
                    <%= succe %> 
                <% }) %> 
            </div>
        </div>
    </div>
<% } %> 